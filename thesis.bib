@article{baumgartner_size_clubs,
abstract = {We study various aspects of the size, including the cardinality, of closed unbounded subsets of [$\lambda$]<$\kappa$, especially when $\lambda$ $\kappa$+n for n $\epsilon${lunate} $\omega$. The problem is resolved into the study of the size of certain stationary sets. Relative to the existence of an $\omega$1-Erd{\"{o}}s cardinal it is shown consistent that $\omega$$\omega$3 < $\omega$$\omega$13 and every closed unbounded subsetof [$\omega$3]<$\omega$2 has cardinality $\omega$$\omega$13. A weakening of the $\omega$1-Erd{\"{o}}s property, $\omega$1-remarkability, is defined and shown to be retained under a large class of Easton-like forcings applied to $\omega$1-Erd{\"{o}}s cardinals. A class of reverse-Easton forcings preserving $\alpha$-Erd{\"{o}}sness is also described, with special attention to the establishment of □-principles. {\textcopyright} 1991.},
author = {Baumgartner, James E.},
doi = {10.1016/0168-0072(91)90047-P},
issn = {01680072},
journal = {Annals of Pure and Applied Logic},
mendeley-groups = {dissertation},
number = {3},
pages = {195--227},
title = {{On the size of closed unbounded sets}},
volume = {54},
year = {1991}
}
