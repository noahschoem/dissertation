@article{levine_sinapova_squares,
abstract = {It is known that if $\kappa$ is inaccessible in V and W is an outer model of V such that ($\kappa$ +) V = ($\kappa$ +) W and cf W ($\kappa$) = $\omega$, then $\kappa$,$\omega$ holds in W. (Many strengthenings of this theorem have been investigated as well.) We show that this theorem does not generalize to uncountable cofinalities: There is a model V in which $\kappa$ is inaccessible, such that there is a forcing extension W of V in which ($\kappa$ +) V = ($\kappa$ +) W and $\omega$ < cf W ($\kappa$) < $\kappa$, while in W , $\kappa$,$\tau$ fails for all $\tau$ < $\kappa$. We make use of Magidor's forcing for singularizing an inaccessible $\kappa$ to have uncountable cofinality. Along the way, we analyze stationary reflection in this model, and we show that it is possible for $\kappa$,cf($\kappa$) to hold in a forcing extension by Magidor's poset if the ground model is prepared with a partial square sequence.},
author = {Levine, Maxwell and Sinapova, Dima},
doi = {10.4064/fm955-9-2020},
file = {:home/nschoem/Documents/library/Levine, Sinapova - Unknown - SQUARES AND UNCOUNTABLY SINGULARIZED CARDINALS.pdf:pdf},
journal = {Fundamenta Mathematicae},
mendeley-groups = {Want to read,dissertation},
pages = {277--296},
title = {{Squares and Uncountably Singularized Cardinals}},
volume = {253},
year = {2021}
}
